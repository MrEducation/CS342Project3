public class Input {

	
	public ArrayList<Blocks> blocks;
	
	//Scanner and File Objects
		static Scanner s; 
		static File f;

		public static void main (String [] args){
			
			
			 JMenuBar frameBar = new JMenuBar();
	          JMenu menu1 = new JMenu("Menu1");
	          JMenuItem menuItem1=new JMenuItem("Menu1_MenuItem1");
	          menu1.add(menuItem1);
	          frameBar.add(menu1);
	          
	          Game gameFrame = new Game();
	          gameFrame.setJMenuBar(frameBar);
	          


			f = new File("input.txt");
	
			try {
				s = new Scanner(f).useDelimiter(" |\n");
			} catch (FileNotFoundException e) {

				e.printStackTrace();
			}
		
	
			int i = 0;
			while(s.hasNext()){
				
				System.out.print(s.next());
	
				
				if(i == 0 || i == 1)
				{
					System.out.println("//////" + s.next());
				}
				
				i++;

			}
			
			System.out.println();
			System.out.println(i);

		}
	
	

}
